class Solution(object):
    def trap(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        h=0
        l=0
        r=len(height)-1
        lm=0
        rm=0
        while(l<r):
            if(height[l]<height[r]):
                if(height[l]>=lm):
                    lm=height[l]
                else:
                    h+=lm-height[l]
                l+=1
            else:
                if(height[r]>=rm):
                    rm=height[r]
                else:
                    h+=rm-height[r]
                r-=1
        return h

        
