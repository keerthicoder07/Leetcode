class Solution {
    public boolean arrayStringsAreEqual(String[] word1, String[] word2) {
        StringBuilder str1=new StringBuilder();
        StringBuilder str2=new StringBuilder();
        for(String X:word1)
        {
            str1.append(X);
        }
        for(String Y:word2)
        {
            str2.append(Y);
        }
        if(str1.toString().equals(str2.toString()))
        return true;
        return false;
    }
}
