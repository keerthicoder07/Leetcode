class Solution {
    public String clearDigits(String s) {
        char[] str=s.toCharArray();
        for(int i=0;i<s.length();i++)
        {
            if(str[i]>=48 && str[i]<=57)
            {
                for(int j=i;j>=0;j--)
                {
                    if(str[j]>=97 && str[j]<=122)
                    {
                            str[j]=' ';
                            str[i]=' ';
                            break;
                    }
                }
            }
            
        }
        s=String.valueOf(str);
        s=s.replace(" ","");
        return s;
    }
}
