class Solution(object):
    def duplicateZeros(self,arr):
        """
        :type arr: List[int]
        :rtype: None Do not return anything, modify arr in-place instead.
        """
        if 0 not in arr:
            return arr
        l=[]
        n=len(arr)
        i=0
        k=0
        while(i<n):
            if arr[i]==0:
                l.append(i+k)
                arr.pop()
                k+=1
                n-=1
            i+=1
        for j in l:
            arr.insert(j,0)
