class Solution(object):
    def findMin(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        s=set(nums)
        nums=list(s)
        print(nums)
        so=sorted(nums)
        n=len(nums)
        nums1=copy.copy(nums)
        mi=100000
        for k in range(n):
            nums=copy.copy(nums1)
            nums=nums[:][::-1]
            nums[:k]=nums[:k][::-1]
            nums[k:]=nums[k:][::-1]
            if(nums[0]<mi):
                mi=nums[0]
        return mi
        


        
