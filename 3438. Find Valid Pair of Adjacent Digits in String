class Solution {
    public String findValidPair(String s) {
        String arr[]=s.split("");
        String r="";
        HashMap<String,Integer> map= new HashMap<>();
        for(String j:arr)
        {
            if(!map.containsKey(j))
            {
                map.put(j,1);
            }
            else
            {
                map.put(j,map.get(j)+1);
            }
        }
        for(int i=0;i<arr.length-1;i++)
        {
        //System.out.println(map.get(arr[i]));
        if((!arr[i].equals(arr[i+1])) && map.get(arr[i]) == Integer.parseInt(arr[i]) && map.get(arr[i+1])==Integer.parseInt(arr[i+1]))
        {
            r=r+arr[i]+arr[i+1];
            break;
        }
           

        }
        return r;
    }
}
