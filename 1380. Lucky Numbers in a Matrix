class Solution {
    public List<Integer> luckyNumbers(int[][] matrix) {
        ArrayList<Integer> list=new ArrayList<>();
        int m=matrix.length;
        int n=matrix[0].length;
        int c=0;
        for(int i=0;i<m;i++)
        {
            int min=matrix[i][0];
            for(int j=1;j<n;j++)
            {
                if(min>matrix[i][j])
                {
                    min=matrix[i][j];
                    c=j;
                }
            }
            int f=0;
            for(int k=0;k<m;k++)
            {
                if(min<matrix[k][c])
                {
                    f=0;
                    break;
                    
                }
                else
                {
                    f=1;
                }
            }
            if(f==1)
            list.add(min);
        }
        //System.out.println(m+" "+n);
        return list;

    }
}
