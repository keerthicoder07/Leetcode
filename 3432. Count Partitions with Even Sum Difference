class Solution(object):
    def countPartitions(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        c=0
        n=len(nums)
        for i in range(1,n):
            sum1=sum(nums[0:i])
            sum2=sum(nums[i:n])
            if((sum1-sum2)%2==0):
                c+=1
        return c

        
