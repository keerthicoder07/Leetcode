class Solution(object):
    def sumOddLengthSubarrays(self, arr):
        """
        :type arr: List[int]
        :rtype: int
        """
        s=0
        n=len(arr)
        for i in range(len(arr)):
            j=i
            while(j<n):
                s+=sum(arr[i:j+1])
                j+=2
        return s

        
