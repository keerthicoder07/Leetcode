class Solution(object):
    def reverseOnlyLetters(self, s):
        """
        :type s: str
        :rtype: str
        """
        if all(not c.isalpha() for c in s):   
            return s
        l=0
        r=len(s)-1
        li=list(s)
        while(l<r):
            while(not s[l].isalpha()):
                l+=1
            while(not s[r].isalpha()):
                r-=1
            if(l<r):
                li[l],li[r]=li[r],li[l]
            l+=1
            r-=1
        return "".join(li)        
