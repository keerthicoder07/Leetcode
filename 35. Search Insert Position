class Solution {
    public int searchInsert(int[] nums, int target) {
        int f=0;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==target)
            {
                f=1;
                return i;
            }
        }
        int j=0;
        if(f!=1)
        {
           
           while(j<nums.length)
           {
               if(target<=nums[j])
               {
                    return j;
               }
               j++;
           }
           
        }
        return j;
    }
}
