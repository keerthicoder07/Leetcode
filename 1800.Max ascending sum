class Solution {
    public int maxAscendingSum(int[] nums) {
        int n=nums.length;
        int sum=0;
        int max=0;
        int i=1;
        if(n==1)
        return nums[0];
        while(i<n)
        {
            sum=nums[i-1];
            while(i<n && nums[i-1]<nums[i])
            {
                sum+=nums[i];
                i++;
            }
            if(sum>max)
            max=sum;
            sum=0;
            i++;
        }
        return max;
    }
}
