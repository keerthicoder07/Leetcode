class Solution {
    public int maxRepeating(String sequence, String word) {
        if (word.length() == 0) return 0;
        int k = 0;
        StringBuilder sb = new StringBuilder();
        while (true) {
            sb.append(word);   
            if (sequence.contains(sb)) {
                k++;
            } else {
                break;
            }
        }
        return k;
    }
}
