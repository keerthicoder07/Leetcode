class Solution {
    public String addStrings(String num1, String num2) {
        if(num1.equals("0"))
        return num2;
        if(num2.equals("0"))
        return num1;
        int l1=num1.length();
        int l2=num2.length();
        int i=l1-1;
        int j=l2-1;
        String res="";
        int sum=0;
        int c=0;
        while(i>=0 || j>=0)
        {
          sum=0;
          sum+=c;
            if(i>=0)
            {
               sum+=num1.charAt(i)-'0';
               i--;
            }
            if(j>=0)
            {
                sum+=num2.charAt(j)-'0';
                j--;
            }
            if(i<0 && j<0)
            {
              res=Integer.toString(sum)+res;
              break;
            }
            if(sum>=10)
            {
            int sum1=sum;
            sum=sum%10;
            c=sum1/10;
            }
            else{
                c=0;
            }
            res=Integer.toString(sum)+res;
        }
        System.out.println(res);
        return res;
    }
    
}
